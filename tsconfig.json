{
	"exclude": ["./node_modules", "./dist", "./test"],
	//"include": ["./src/**/*", "./src/*"],
	"compilerOptions": {
		/* Lib */
		"lib": ["ES6", "dom"],
		"target": "ES6",
		"module": "CommonJS",
		"skipLibCheck": true,
		/* Out */
		"rootDir": "./",
		"outDir": "./dist",
		/* Compiler options */
		"allowJs": true,
		"declaration": true,
		"esModuleInterop": true,
		"pretty": true
	}
}
